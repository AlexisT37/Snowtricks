{% extends 'base.html.twig' %}

{% block title %} Une figure de snowboard | {{ parent() }}{% endblock %}

{% block body %}
<h1>{{ title }}</h1>
    <ul>
        {% for trick in tricks %}
            <li>
                {{ trick.title }} - {{ trick.content }}
            </li>
        {% endfor %}
    </ul>
{% endblock %}

